                    <MenuItem.Icon>
                        <CheckBox  Checked="cpCOmini_CheckBox_Checked" Unchecked="cpCOmini_CheckBox_Unchecked"/>
                    </MenuItem.Icon>
                </MenuItem>
                <MenuItem Header="cpCOmini">
                    <MenuItem.Icon>
                        <CheckBox Checked="Visibility_CheckBox_Checked" Unchecked="Visibility_CheckBox_Unchecked"/>
                    </MenuItem.Icon>
                </MenuItem>
            </MenuItem>
            <MenuItem Header="Сеть">
